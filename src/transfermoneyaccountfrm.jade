extends jade/template
block includes
	include jade/mixins/**/*.jade
block title
	title 123
block content
	.container
		form.pull-right(style='width:100%').text-center
			+datepange
		.row
			.form-input.text-center
			.form-group
				.row
					.col-sm-11
						select(name='1')#select2(style='width:100%').form-control
							option(value='1') 1
							option(value='2') 2
					label.control-label(for="select2") :الى حساب
			.form-group
				.row
					.col-sm-11
						select(name='1')#select1(style='width:100%').form-control
							option(value='1') 1
							option(value='2') 2
					label.control-label(for="select1") :من حساب

		.row
			.form-group.col-sm-6
				.input-group
					span.input-group-addon
						i.fa.fa-search
					input(type='text').form-control
			.form-group.col-sm-6
				+btnprint
				+btnsave
				+btncross
				+btnsucc


	.container
		table.table.table-responsive.table-bordered.table-striped(style='width:100%')
			thead
				tr
					th البيان
					th بيان الدفع
					th تاريخ التحويل
					th المبلغ
					th الى حساب
					th من حساب
					th المستلم

			tbody
				tr
					td 1
					td 2
					td 3
					td 4
					td 5
					td 6
					td 7



