extends jade/template
block includes
   include jade/mixins/**/*.jade
block title
   title 123
block content
   .container
      form(role="form")
         +datepange
         .form-group
            .col-sm-10
               input(type='text' id='input1').form-control
            label(for='input1') :اسم العميل او اي مقطع منه
         .form-input.text-center
            +checkbox('1', 'بحث بالتاريخ لاخر دفعة', '1')

         .form-group
            .row
               .col-sm-11
                  select(name='1')#select1(style='width:100%').form-control
                     option(value='1') 1
                     option(value='2') 2
               label.control-label(for="select1") :الدولة
         .form-group
            .row
               .col-sm-11
                  select(name='1')#select3(style='width:100%').form-control
                     option(value='1') 1
                     option(value='2') 2
               label.control-label(for="select3") : Nationality
         .form-group
            .row
               .col-sm-11
                  select(name='1')#select2(style='width:100%').form-control
                     option(value='1') 1
                     option(value='2') 2
               label.control-label(for="select2") المستخدم
         .form-input.text-center
            +checkbox('2', 'العملاء المسددين', '2')
            +checkbox('3', 'عقود منتهية', '3')
         .form-group(style="margin-top:20px")
            .row.text-center
               .col-sm-6
                  +button('طباعة')
               .col-sm-6
                  +button('بحث')
   .container
